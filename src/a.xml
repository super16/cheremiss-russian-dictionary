  <entry>
    <form>
      <orth>Ава́</orth>
    </form>
    <etym>
      <usg type="lang">чув.</usg>
      <lang>чув.</lang>
      <mentioned>аба</mentioned>
    </etym>
    <sense>
      <cit type="translation" xml:lang="ru-petr1708">
        <quote>мать самка</quote>
      </cit>
      <cit type="example" xml:lang="chm">
        <quote>ава́й</quote>
        <cit type="translation" xml:lang="ru-petr1708">
          <quote>мати моя</quote>
        </cit>
      </cit>
      <cit type="example" xml:lang="chm">
        <quote>ава́ вель</quote>
        <etym>
          <usg type="lang">г.чер.</usg>
          <lang>г.чер.</lang>
          <mentioned>вара́ш абя́</mentioned>
        </etym>
        <cit type="translation" xml:lang="ru-petr1708">
          <quote>мачиха</quote>
        </cit>
      </cit>
      <cit type="example" xml:lang="chm">
        <quote>ава́-вель-шу́до</quote>
        <cit type="translation" xml:lang="ru-petr1708">
          <quote>трава мать и мачеха, бѣлокопытникъ</quote>
        </cit>
      </cit>
      <cit type="example" xml:lang="chm">
        <quote>крес-ава́</quote>
        <cit type="translation" xml:lang="ru-petr1708">
          <quote>воспріемница</quote>
        </cit>
      </cit>
    </sense>
